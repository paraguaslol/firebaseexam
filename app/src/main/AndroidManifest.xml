<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dmb.quicktrade">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Users.UsersActivity" />
        <activity android:name=".Users.RegisterActivity" />
        <activity android:name=".Users.LoginActivity" />
        <activity android:name=".Users.ShowUsersActivity" />
        <activity android:name=".Users.EditUserActivity" />
        <activity android:name=".Products.AddProductActivity" />
        <activity android:name=".Products.ProductsActivity"></activity>
    </application>

</manifest>